# cook your dish here
for _ in range(int(input())):
    n= int(input())
    a= list(map(int, input().split()))
    m= int(input())
    b= list(map(int, input().split()))
    l= []
    for i in range(n):
        for j in range(m):
            if b[j]==a[i]:
                l.append(a[i])
                break
    if l==b:
        print('Yes')
    else:
        print('No')
