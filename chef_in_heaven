for _ in range(int(input())):
    l = int(input())
    s = input()[:l]
    f=0
    one,zero =0,0
    for i in range(len(s)):
        if(s[i]=="0"):
            zero+=1
        else:
            one+=1

        if(one>=zero):
            f=1
            break
    if(f==0):
        print("NO")
    else:
        print("YES")
